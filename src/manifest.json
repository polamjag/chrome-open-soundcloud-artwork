{
  "name" : "Open SoundCloud Coverart",
  "version" : "0.1.0",
  "manifest_version": 2,
  "permissions" : [
    "contextMenus",
    "https://soundcloud.com/*"
  ],
  "content_scripts":[ {
    "matches": ["https://soundcloud.com/*"],
    "js": [
      "js/content.js"
    ]
  }],
  "background" : {
    "scripts": [
      "js/main.js"
    ]
  },
  "icons" : {
    "16" : "icon-128.png",
    "128" : "icon-128.png"
  }
}
